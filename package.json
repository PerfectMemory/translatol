{
  "name": "translatol",
  "description": "A tool in order to translate XLIFF files",
  "author": "Vincent Tabary <vincent@tabary.me>",
  "version": "0.1.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/vtabary/translatol"
  },
  "engines": {
    "node": ">=14.15"
  },
  "homepage": "https://github.com/vtabary/translatol",
  "license": "ISC",
  "scripts": {
    "ng": "ng",
    "start": "nx serve translatol-electron-app",
    "build": "nx build  translatol-electron-app",
    "build:prod": "nx build  translatol-electron-app --configuration production",
    "test": "nx test translatol-electron-app",
    "lint": "nx lint translatol-electron-app",
    "e2e": "nx e2e translatol-electron-app",
    "electron": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder build",
    "postinstall": "electron-builder install-app-deps",
    "prepare": "husky install",
    "postversion": "git push origin HEAD && git push origin v$npm_package_version",
    "build:extension": "nx build translatol-vscode-extension && nx build translatol-vscode-webview"
  },
  "main": "electron.js",
  "build": {
    "appId": "com.vtabary.translatol",
    "productName": "Translatol",
    "copyright": "Copyright Â© 2021 Vincent Tabary",
    "files": [
      "dist/translatol-electron-app",
      "src/electron",
      "electron.js"
    ],
    "directories": {
      "buildResources": "./build",
      "output": "./dist/packages/${os}/${arch}"
    },
    "win": {
      "icon": "./build/icon.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "nsis": {},
    "linux": {
      "icon": "./build/icon.png",
      "category": "Utility",
      "target": [
        {
          "target": "deb",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "publish": [
        {
          "provider": "github"
        }
      ]
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.7",
    "@angular/cdk": "^15.2.7",
    "@angular/common": "^15.2.7",
    "@angular/compiler": "^15.2.7",
    "@angular/core": "^15.2.7",
    "@angular/forms": "^15.2.7",
    "@angular/platform-browser": "^15.2.7",
    "@angular/platform-browser-dynamic": "^15.2.7",
    "@angular/router": "^15.2.7",
    "@cds/core": "^6.4.0",
    "@clr/angular": "15.2.0",
    "@clr/ui": "^15.2.0",
    "@electron/remote": "^2.0.0",
    "@vtabary/xliff2js": "^0.3.1",
    "@webcomponents/custom-elements": "^1.0.0",
    "axios": "^1.0.0",
    "fast-glob": "^3.2.7",
    "lodash": "^4.17.15",
    "rxjs": "~6.6.7",
    "stream": "^0.0.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.6",
    "@angular-devkit/core": "^15.2.6",
    "@angular-devkit/schematics": "^15.2.6",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.6",
    "@angular/compiler-cli": "^15.2.7",
    "@angular/language-service": "^15.2.7",
    "@angular/localize": "^15.2.8",
    "@nrwl/angular": "15.9.2",
    "@nrwl/cypress": "15.9.2",
    "@nrwl/esbuild": "15.9.2",
    "@nrwl/eslint-plugin-nx": "15.9.2",
    "@nrwl/jest": "15.9.2",
    "@nrwl/js": "15.9.2",
    "@nrwl/node": "^15.9.2",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/web": "15.9.2",
    "@nrwl/workspace": "15.9.2",
    "@schematics/angular": "^15.2.6",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.172",
    "@types/node": "^18.11.18",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "@vscode/test-electron": "^2.3.0",
    "cypress": "^12.2.0",
    "electron": "^14.0.0",
    "electron-builder": "^22.0.0",
    "esbuild": "^0.17.5",
    "eslint": "^8.28.0",
    "eslint-plugin-cypress": "^2.10.3",
    "husky": ">=6",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "jest-preset-angular": "~13.0.0",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": ">=10",
    "ng-packagr": "^15.2.2",
    "nx": "15.9.2",
    "prettier": "^2.6.2",
    "protractor": "~7.0.0",
    "ts-jest": "^29.0.5",
    "ts-node": "10.9.1",
    "tslint": "~6.1.0",
    "typescript": "~4.9.5"
  }
}
