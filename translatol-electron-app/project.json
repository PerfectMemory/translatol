{
  "name": "translatol-electron-app",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "generators": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "sourceRoot": "translatol-electron-app/src",
  "prefix": "app",
  "tags": ["scope:app"],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/translatol-electron-app",
        "index": "translatol-electron-app/src/index.html",
        "main": "translatol-electron-app/src/main.ts",
        "polyfills": "translatol-electron-app/src/polyfills.ts",
        "tsConfig": "translatol-electron-app/tsconfig.app.json",
        "assets": ["translatol-electron-app/src/favicon.ico", "translatol-electron-app/src/assets"],
        "styles": ["node_modules/@clr/ui/clr-ui.min.css", "translatol-electron-app/src/styles.scss"],
        "scripts": ["node_modules/@webcomponents/custom-elements/custom-elements.min.js"],
        "vendorChunk": true,
        "extractLicenses": false,
        "buildOptimizer": false,
        "sourceMap": true,
        "optimization": false,
        "namedChunks": true
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "translatol-electron-app/src/environments/environment.ts",
              "with": "translatol-electron-app/src/environments/environment.prod.ts"
            }
          ],
          "optimization": false,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": false,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "10mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "6kb",
              "maximumError": "10kb"
            }
          ]
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": false,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "translatol-electron-app:build:production"
        },
        "development": {
          "browserTarget": "translatol-electron-app:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "translatol-electron-app:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["translatol-electron-app/**/*.ts", "translatol-electron-app/**/*.html"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "translatol-electron-app/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "serve-static": {
      "executor": "@nrwl/web:file-server",
      "options": {
        "buildTarget": "translatol-electron-app:build"
      }
    }
  }
}
